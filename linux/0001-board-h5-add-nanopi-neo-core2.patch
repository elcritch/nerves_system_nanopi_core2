+/dts-v1/;
+#include "sun50i-h5.dtsi"
+
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/input/input.h>
+#include <dt-bindings/pinctrl/sun4i-a10.h>
+
+/ {
+	model = "FriendlyARM NanoPi NEO Core2";
+	compatible = "friendlyarm,nanopi-neo-core2", "allwinner,sun50i-h5";
+
+	aliases {
+		ethernet0 = &emac;
+		serial0 = &uart0;
+	};
+
+	chosen {
+		stdout-path = "serial0:115200n8";
+	};
+
+	leds {
+		compatible = "gpio-leds";
+       pinctrl-names = "default";
+       pinctrl-0 = <0x3c 0x3d>;
+
+       pwr {
+               label = "nanopi:green:pwr";
+               gpios = <0x3e 0x0 0xa 0x0>;
+               default-state = "on";
+       };
+       status {
+               label = "nanopi:blue:status";
+               gpios = <0x17 0x0 0xa 0x0>;
+               linux,default-trigger = "heartbeat";
+       };
+	};
+
+	reg_gmac_3v3: gmac-3v3 {
+		compatible = "regulator-fixed";
+		pinctrl-names = "default";
+		regulator-name = "gmac-3v3";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		startup-delay-us = <100000>;
+		enable-active-high;
+		gpio = <&pio 3 6 GPIO_ACTIVE_HIGH>;
+	};
+
+	reg_vcc3v3: vcc3v3 {
+		compatible = "regulator-fixed";
+		regulator-name = "vcc3v3";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+	};
+
+	wifi_pwrseq: wifi_pwrseq {
+		compatible = "mmc-pwrseq-simple";
+		pinctrl-names = "default";
+		reset-gpios = <&r_pio 0 7 GPIO_ACTIVE_LOW>; /* PL7 */
+		post-power-on-delay-ms = <200>;
+	};
+
+   ths@01c25000 {
+       #thermal-sensor-cells = <0x0>;
+       compatible = "allwinner,sun8i-h3-ths";
+       reg = <0x1c25000 0x400 0x1c14234 0x4>;
+       interrupts = <0x0 0x1f 0x4>;
+       resets = <0x4 0x2a>;
+       reset-names = "ahb";
+       clocks = <0x4 0x37 0x4 0x45>;
+       clock-names = "ahb", "ths";
+       linux,phandle = <0x35>;
+       phandle = <0x35>;
+   };
+
+   timer@01c20c00 {
+       compatible = "allwinner,sun4i-a10-timer";
+       reg = <0x1c20c00 0xa0>;
+       interrupts = <0x0 0x12 0x4 0x0 0x13 0x4>;
+       clocks = <0x1b>;
+   };
+   thermal-zones {
+
+       cpu_thermal {
+               polling-delay-passive = <0x14a>;
+               polling-delay = <0x3e8>;
+               thermal-sensors = <0x35 0x0>;
+               linux,phandle = <0x77>;
+               phandle = <0x77>;
+
+               trips {
+
+                       cpu_warm {
+                               temperature = <0xfde8>;
+                               hysteresis = <0x7d0>;
+                               type = "passive";
+                               linux,phandle = <0x36>;
+                               phandle = <0x36>;
+                       };
+
+                       cpu_hot {
+                               temperature = <0x124f8>;
+                               hysteresis = <0x7d0>;
+                               type = "passive";
+                               linux,phandle = <0x38>;
+                               phandle = <0x38>;
+                       };
+
+                       cpu_very_hot {
+                               temperature = <0x15f90>;
+                               hysteresis = <0x7d0>;
+                               type = "passive";
+                               linux,phandle = <0x39>;
+                               phandle = <0x39>;
+                       };
+
+                       cpu_crit {
+                               temperature = <0x19a28>;
+                               hysteresis = <0x7d0>;
+                               type = "critical";
+                               linux,phandle = <0x78>;
+                               phandle = <0x78>;
+                       };
+               };
+
+               cooling-maps {
+
+                       cpu_warm_limit_cpu {
+                               trip = <0x36>;
+                               cooling-device = <0x37 0xffffffff 0xffffffff>;
+                       };
+
+                       cpu_hot_limit_cpu {
+                               trip = <0x38>;
+                               cooling-device = <0x37 0xffffffff 0xffffffff>;
+                       };
+
+                       cpu_very_hot_limit_cpu {
+                               trip = <0x39>;
+                               cooling-device = <0x37 0x12 0xffffffff>;
+                       };
+               };
+       };
+   };
+
+};
+
+&cpu0 {
+	cpu-supply = <&vdd_cpux>;
+};
+
+&codec {
+	allwinner,audio-routing =
+		"Line Out", "LINEOUT",
+		"MIC1", "Mic",
+		"Mic",  "MBIAS";
+	status = "okay";
+};
+
+&ehci0 {
+	status = "okay";
+};
+
+&ehci3 {
+	status = "okay";
+};
+
+/*&mdio {
+	ext_rgmii_phy: ethernet-phy@1 {
+		reg = <0>;
+	};
+};*/
+
+&emac {
+	pinctrl-names = "default";
+	pinctrl-0 = <&emac_rgmii_pins>;
+	phy-supply = <&reg_gmac_3v3>;
+	phy-handle = <&ext_rgmii_phy>;
+	phy-mode = "rgmii";
+	local-mac-address = [ 00 00 00 00 00 00 ];
+
+	allwinner,leds-active-low;
+	status = "okay";
+};
+
+&external_mdio {
+	ext_rgmii_phy: ethernet-phy@7 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <7>;
+	};
+};
+
+&mmc0 {
+	vmmc-supply = <&reg_vcc3v3>;
+	bus-width = <4>;
+	cd-gpios = <&pio 5 6 GPIO_ACTIVE_LOW>; /* PF6 */
+	status = "okay";
+};
+
+&mmc1 {
+	vmmc-supply = <&reg_vcc3v3>;
+	vqmmc-supply = <&reg_vcc3v3>;
+	mmc-pwrseq = <&wifi_pwrseq>;
+	bus-width = <4>;
+	non-removable;
+	status = "okay";
+
+	brcmf: wifi@1 {
+		reg = <1>;
+		compatible = "brcm,bcm4329-fmac";
+	};
+};
+
+&mmc2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&mmc2_8bit_pins>;
+	vmmc-supply = <&reg_vcc3v3>;
+	bus-width = <8>;
+	non-removable;
+	cap-mmc-hw-reset;
+	status = "okay";
+};
+
+&ohci0 {
+	status = "okay";
+};
+
+&ehci0 {
+	status = "okay";
+};
+
+&ohci1 {
+	status = "okay";
+};
+
+&ehci1 {
+	status = "okay";
+};
+
+&ohci2 {
+	status = "okay";
+};
+
+&ehci2 {
+	status = "okay";
+};
+
+&ohci3 {
+	status = "okay";
+};
+
+&ehci3 {
+	status = "okay";
+};
+
+&r_i2c {
+	status = "okay";
+
+	vdd_cpux: regulator@65 {
+		compatible = "silergy,sy8106a";
+		reg = <0x65>;
+		silergy,fixed-microvolt = <1000000>;
+		regulator-min-microvolt = <1000000>;
+		regulator-max-microvolt = <1400000>;
+		regulator-ramp-delay = <200>;
+		regulator-boot-on;
+		regulator-always-on;
+	};
+};
+
+&uart0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart0_pins_a>;
+	status = "okay";
+};
+
+&uart1 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart1_pins>;
+	status = "okay";
+};
+
+&spi0 {
+	status = "okay";
+};
+
+/* &usb_otg {
+	dr_mode = "host";
+	status = "okay";
+}; */
+
+&usbphy {
+	/* USB Type-A ports' VBUS is always on */
+	status = "okay";
+};
+
